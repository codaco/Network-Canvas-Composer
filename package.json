{
    "name": "network-canvas-architect",
    "version": "4.0.0-alpha.3.b",
    "productName": "Network Canvas Architect",
    "description": "A tool for creating Network Canvas interviews.",
    "author": "Complex Data Collective",
    "private": true,
    "engines": {
        "node": "8.11.1",
        "npm": "5.8.0"
    },
    "scripts": {
        "pack": "electron-builder --dir",
        "start": "cross-env BUILD_TARGET=web node scripts/start.js",
        "start:electron": "node scripts/start.js",
        "preelectron:dev": "node scripts/build-electron-dev.js",
        "electron:dev": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_PORT=3003 electron electron-dev/",
        "build": "node scripts/build.js",
        "lint": "eslint src",
        "lint:sass": "sass-lint -vc .sass-lint.yml",
        "test": "node scripts/test.js --env=jsdom",
        "test-update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
        "build-docs": "jsdoc src -r -d docs-build -c ./jsdoc.conf.json --verbose",
        "electron": "electron ./www",
        "generate-icons": "node scripts/generate-app-icons.js",
        "dist": "electron-builder",
        "dist:mac": "npm run build && electron-builder -m --publish=always",
        "dist:linux": "npm run build && electron-builder -l --publish=always",
        "dist:win": "npm run build && electron-builder -w --publish=always",
        "dist:all": "npm run build && electron-builder -mlw --publish=always",
        "bump": "sh scripts/bump",
        "preflight": "npm run lint && npm run lint:sass -- --max-warnings 0 && npm test -- --coverage"
    },
    "repository": {
        "type": "git"
    },
    "devDependencies": {
        "animejs": "^2.2.0",
        "archiver": "^2.1.1",
        "autoprefixer": "7.1.2",
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "^7.1.5",
        "babel-preset-react-app": "^3.1.2",
        "babel-preset-stage-3": "^6.24.1",
        "babel-runtime": "6.26.0",
        "brfs": "^1.4.3",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "classnames": "^2.2.6",
        "color": "^2.0.0",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "decompress": "^4.2.0",
        "dotenv": "4.0.0",
        "electron": "^2.0.4",
        "electron-builder": "^20.19.2",
        "electron-installer-dmg": "^0.2.0",
        "electron-log": "^2.2.16",
        "electron-packager": "^8.5.2",
        "electron-to-chromium": "^1.3.52",
        "electron-updater": "^2.23.3",
        "electron-winstaller": "^2.5.2",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.2",
        "es6-promise-plugin": "^4.1.0",
        "eslint": "^4.8.0",
        "eslint-config-airbnb": "^15.0.1",
        "eslint-config-react-app": "^2.0.1",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-flowtype": "2.35.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "^7.10.0",
        "file-loader": "^1.1.11",
        "fs-extra": "3.0.1",
        "history": "^4.7.2",
        "html-webpack-plugin": "^3.2.0",
        "icon-gen": "^1.2.1",
        "jest": "^20.0.4",
        "jsdoc": "^3.4.3",
        "jsdoc-babel": "^0.3.0",
        "json-loader": "^0.5.7",
        "lodash": "^4.17.10",
        "mini-css-extract-plugin": "^0.4.1",
        "mkdirp": "^0.5.1",
        "network-canvas-ui": "git+https://github.com/codaco/Network-Canvas-UI.git#1.13.3",
        "node-sass": "^4.9.2",
        "object-assign": "4.1.1",
        "path-to-regexp": "^2.2.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "^2.1.6",
        "promise": "7.1.1",
        "prop-types": "^15.6.2",
        "query-string": "^6.1.0",
        "react": "^16.4.1",
        "react-color": "^2.14.1",
        "react-dev-utils": "^6.0.0-next.3e165448",
        "react-dom": "^16.4.1",
        "react-dropzone": "^4.2.13",
        "react-fastclick": "^3.0.2",
        "react-markdown": "^3.3.4",
        "react-redux": "^4.4.5",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^1.1.4",
        "react-sortable-hoc": "^0.6.8",
        "react-transition-group": "^2.4.0",
        "recompose": "^0.26.0",
        "redux": "^3.6.0",
        "redux-form": "^7.4.2",
        "redux-logger": "^2.7.4",
        "redux-mock-store": "^1.5.3",
        "redux-observable": "^0.14.1",
        "redux-persist": "^4.10.1",
        "redux-thunk": "^2.3.0",
        "redux-undo": "^0.6.1",
        "reselect": "^3.0.0",
        "rxjs": "^5.5.11",
        "rxjs-marbles": "^2.1.0",
        "sass-lint": "^1.12.0",
        "sass-loader": "^7.0.3",
        "sass-svg": "^1.0.1",
        "style-loader": "^0.21.0",
        "svg2png": "^4.1.1",
        "url-loader": "^1.0.1",
        "uuid": "^3.3.2",
        "webpack": "^4.16.1",
        "webpack-dev-server": "^3.1.4",
        "webpack-manifest-plugin": "^2.0.3",
        "whatwg-fetch": "2.0.3"
    },
    "dependencies": {
        "react-error-overlay": "^5.0.0-next.3e165448",
        "thread-loader": "^1.1.5"
    },
    "homepage": ".",
    "main": "www/electron-starter.js",
    "browserslist": [
        "Chrome 61"
    ],
    "build": {
        "extends": null,
        "appId": "NetworkCanvasArchitect",
        "directories": {
            "buildResources": "build-resources",
            "output": "release-builds"
        },
        "mac": {
            "category": "public.app-category.education"
        },
        "win": {
            "target": "nsis",
            "certificateSubjectName": "Complex Data Collective"
        },
        "fileAssociations": {
            "ext": "netcanvas",
            "description": "Network Canvas interview protocol",
            "icon": "file",
            "role": "Editor"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "codaco",
                "repo": "architect"
            }
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/ui/**"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/config/jest/polyfills.js",
            "<rootDir>/config/jest/enzyme.js",
            "<rootDir>/config/jest/automock.js"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>[/\\\\](www|docs|node_modules|scripts)[/\\\\]"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        }
    }
}
